<!-- 请上传图像上半部分界面设计 -->

<view>
    <view class="number1">
      <view class="font1" >请上传图像…</view>
      <image class="download" src="{{tempfilepathimg}}" style="width: 100%; height: 100%"/>
    </view>
</view>

<!-- 设置三个按钮样式 -->
<view class="click_btn">
  <view class="btn1"><button size="mini" type="primary" class="upimg" bindtap="send">加载图像</button></view>
  <view class="btn2"><button size="mini" type="primary" class="clear" bindtap="click">识别</button></view>
  <view class="btn3"><button size="mini" type="primary" class="clear" bindtap="clear">清除</button></view>
</view>

<!-- 设置返回结果 -->
<view class="src" bindtap="xiangqin">
<view class="food" >名称：{{food}}</view>
<view class="time" >耗时：{{time}}</view>
</view>
